{
  "name": "randomCross",
  "description": "Hightlight some pre defined text from websql database after page loads",
  "version": "0.1",
  "permissions": [
      "tabs","<all_urls>"
  ],
  "browser_action": {
      "default_icon": "128.png",
      "default_popup": "index.html"
  },

  "content_scripts": [
      {
      "matches": [
          "http://*/*",
          "https://*/*"
      ],
      "js": ["assest/index.js"]
      }
  ],
  "permissions": [
    "tabs","<all_urls>"
],

  "manifest_version": 2
}